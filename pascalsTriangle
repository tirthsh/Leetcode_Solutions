class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
    
        triangle = []
        if (numRows == 0):
            return triangle
        
        #first row is always 1
        frow = [1]
        triangle.append(frow)
        
        for i in range(1, numRows):
            prevRow = triangle[i - 1]
            currRow = [1]
            
            for j in range(1, i):
                currRow.append(prevRow[j - 1] + prevRow[j])
            
            currRow.append(1)
            
            triangle.append(currRow)
        
        return triangle
